{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "esNext",
      "target": "es5",
      "jsx": "react-jsx",
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["./src/*"]
      },
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
    }
  }